        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="POST - Register Client" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import oauth.signpost.OAuth;
import oauth.signpost.OAuthConsumer;
import oauth.signpost.OAuthProvider;
import oauth.signpost.basic.DefaultOAuthConsumer;
import oauth.signpost.basic.DefaultOAuthProvider;

import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.net.HttpURLConnection;
import java.net.URL;
import groovy.json.JsonSlurper 

OAuthConsumer consumer = new DefaultOAuthConsumer(vars.get(&quot;accesskey&quot;),
     vars.get(&quot;secretkey&quot;));
URL url = new URL(vars.get(&quot;albURL&quot;) + &quot;/authorizationserver/as/v1/api/clients/&quot;+ vars.get(&quot;clientType&quot;));

HttpURLConnection request = (HttpURLConnection) url.openConnection();
request.with {
  doOutput = true
  requestMethod = &apos;POST&apos;
}
consumer.sign(request);
request.connect();
BufferedReader reader = new BufferedReader(new InputStreamReader(
request.getInputStream()));
String inputLine;

while ((inputLine = reader.readLine()) != null)
{
   log.info(inputLine);
    def jsonSlurper = new JsonSlurper();  // use for the parse(seperate)client id and client secret 
    def object = jsonSlurper.parseText(inputLine) ;
   vars.put(&quot;Client_Id&quot;, object.client_id);    // put the value of client id from log viewer to the variable client id
   vars.put(&quot;Client_Secret&quot;, object.client_secret);  // put the value of client secret from log viewer to the variable client secret
}

//log.info(vars.get(&quot;Client_Id&quot;)); to check client id
//log.info(vars.get(&quot;Client_Secret&quot;)); to check client secret


reader.close();
request.disconnect();


</stringProp>
